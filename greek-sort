# test cases
# greek_comparator(alpha, beta) --> 1
# greek_comparator(chi, chi) --> 0 
# greek_comparator(upsilon, rho) --> -3


def greek_comparator(lhs, rhs):
    greek_alphabet = ('alpha','beta','gamma','delta','epsilon',
'zeta','eta','theta','iota','kappa','lambda','mu','nu','xi','omicron','pi','rho','sigma','tau',
'upsilon','phi','chi','psi','omega')
    lhs = greek_alphabet.index(lhs)
    rhs = greek_alphabet.index(rhs)
    return lhs-rhs
